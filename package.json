{
  "name": "gptbots-assistant",
  "version": "1.0.1",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "build:dev": "rsbuild build --env-mode dev",
    "build:prod": "rsbuild build --env-mode prod",
    "dev": "rsbuild dev --env-mode dev --open",
    "format": "prettier --write .",
    "lint": "eslint .",
    "preview": "rsbuild preview",
    "zip:chrome": "date1=$(date \"+%m%d%H%M\" ) && zip -r ./gptbots-assistant-chrome-prod-$date1.zip ./chrome -x node_modules/* -x *.zip -x *.git* -x *.DS_Store"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@microsoft/fetch-event-source": "^2.0.1",
    "ant-design-vue": "^3.2.20",
    "axios": "^1.9.0",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.3"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.3.1",
    "@rsbuild/plugin-less": "^1.2.2",
    "@rsbuild/plugin-vue": "^1.0.7",
    "@types/chrome": "^0.0.317",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^22.15.2",
    "@vue/eslint-config-typescript": "^14.5.0",
    "archiver": "^6.0.1",
    "aws-sdk": "^2.1692.0",
    "eslint": "^9.23.0",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.29.0"
  }
}